{
  "compilerOptions": {
    "target": "ESNext",
    "rootDir": "./",
    // "rootDir": ".",
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "./dist",
    "module": "CommonJS",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["./**/*", "dist"],
  "exclude": ["node_modules"]
}
